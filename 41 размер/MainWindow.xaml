<Window x:Class="_41_размер.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_41_размер"
        mc:Ignorable="d"
        Title="Ямалетдинов 41 размер" Height="450" Width="800" MinHeight="50" MinWidth="50" FontFamily=" Comic Sans MS" Icon="Res/icon.ico" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <TextBlock Text="41 размер" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#FFCC6600" />
        <Image Source="Res/logo.png" HorizontalAlignment="Left"/>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Name="Back" Content="Назад" FontSize="15" HorizontalAlignment="Right" VerticalAlignment="Top" Click="Back_Click"/>
    </Grid>
</Window>
